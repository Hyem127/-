<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_cute_fox"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cute_fox"
        android:supportsRtl="true"
        android:theme="@style/Theme.OTTProject"
        tools:targetApi="31">
        <activity
            android:name=".FirstActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />

        </activity>
        <activity android:name=".MainActivity" android:label="MainActivity"/>
        <activity android:name=".LoginActivity" android:label ="LoginActivity"/>
        <activity android:name=".JoinActivity" android:label ="JoinActivity"/>
        <activity android:name=".ContentsActivity" android:label ="ContentsActivity"/>
        <activity android:name=".Contents_Information" android:label="Contents_Information"/>
        <activity android:name=".Community_Write" android:label="Contents_Write"/>
        <activity android:name=".OTT_Ranking" android:label="OTT_Ranking"/>
        <activity android:name=".My_Contents_List" android:label="My_Contents_List"/>
        <activity android:name=".My_Write_List" android:label="My_Write_List"/>
        <activity android:name=".Recommend_All" android:label="Recommend_All"/>
    </application>

</manifest>